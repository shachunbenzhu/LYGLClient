			        android:button="@null"
			        android:layout_weight="1"
			        android:drawableTop="@drawable/qitaorder_yl"
			        android:text="@string/yl"
			        android:textSize="13sp"/>
			</RadioGroup>
		<RelativeLayout 
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:padding="5dp"
	        android:layout_marginTop="5dp">
	        <TextView 
	            android:layout_width="125dp"
	            android:layout_height="1dp"
	            android:layout_centerVertical="true"
	            android:background="@drawable/qgray"
	            android:padding="3dp"/>
	        
	        <com.cn.ncist.rq.module.ChildTextView 
	            android:id="@+id/tv_qitaorder_cxbb"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_centerVertical="true"
	            android:layout_centerHorizontal="true"
	            android:text="@string/cxbb"
	            android:textSize="15sp"
	            android:padding="2dp"/>
	        
	        <TextView 
	            android:layout_width="115dp"
	            android:layout_height="1dp"
	            android:layout_centerVertical="true"
	            android:background="@drawable/qgray"
	            android:padding="3dp"
	            android:layout_marginLeft="8dp"
	            android:layout_toRightOf="@id/tv_qitaorder_cxbb"/>
	    </RelativeLayout>
	        <RadioGroup 
			    android:layout_width="match_parent"
			    android:layout_height="80dp"
			    android:orientation="horizontal"
			    android:id="@+id/rg_qitaorder_cxbb"
			    android:gravity="center">
			    
			    <RadioButton 
			        android:layout_width="0dp"
			        android:layout_height="match_parent"
			        android:id="@+id/rb_qitaorder_wf"
			        android:gravity="center"
			        android:button="@null"
			        android:checked="true"
			        android:layout_weight="1"
			        android:drawableTop="@drawable/qitaorder_wf"
			        android:text="@string/wf"
			        android:textSize="13sp"/>
			    
			    <RadioButton 
			        android:layout_width="0dp"
			        android:layout_height="match_parent"
			        android:id="@+id/rb_qitaorder_bx"
			        android:gravity="center"
			        android:button="@null"
			        android:layout_weight="1"
			        android:drawableTop="@drawable/qitaorder_bx"
			        android:text="@string/bx"
			        android:textSize="13sp"/>
			    
			    <RadioButton 
			        android:layout_width="0dp"
			        android:layout_height="match_parent"
			        android:id="@+id/rb_qitaorder_ypsc"
			        android:gravity="center"
			        android:button="@null"
			        android:layout_weight="1"
